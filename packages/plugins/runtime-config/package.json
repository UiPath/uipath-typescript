{
  "name": "@uipath/apps-runtime-config",
  "version": "1.0.0-dev.2",
  "description": "Universal bundler plugin for UiPath SDK configuration injection",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "unplugin": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "tsdown": "^0.12.5",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vite": ">=4.0.0",
    "webpack": ">=5.0.0",
    "rollup": ">=3.0.0",
    "esbuild": ">=0.17.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UiPath/uipath-typescript.git",
    "directory": "packages/apps-runtime-config"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
