{
    "name": "@uipath/uipath-typescript",
    "version": "1.0.0-beta.8",
    "description": "UiPath TypeScript SDK",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "clean": "rimraf dist && npm run clean --workspaces",
        "prebuild": "npm run clean",
        "build": "tsc && npm run build --workspaces",
        "build:sdk": "tsc",
        "lint": "eslint src/**/*.ts && npm run lint --workspaces",
        "docs:api": "typedoc",
        "verify": "./scripts/verify-packages.sh"
    },
    "dependencies": {
        "@opentelemetry/sdk-logs": "^0.204.0",
        "axios": "^1.6.7",
        "eventemitter3": "^5.0.1",
        "file-type": "^21.0.0",
        "mime-types": "^3.0.1",
        "uuid": "^9.0.1",
        "ws": "^8.16.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/mime-types": "^3.0.1",
        "@types/node": "^20.11.19",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "dotenv": "^17.2.0",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.4.0",
        "typedoc": "^0.28.13",
        "typedoc-plugin-markdown": "^4.8.1",
        "typescript": "^5.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UiPath/uipath-typescript.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "workspaces": [
        "packages/*"
    ]
}
