{
    "name": "@uipath/uipath-typescript",
    "version": "1.0.0-beta.12",
    "description": "UiPath TypeScript SDK",
    "license": "MIT",
    "keywords": [
        "uipath",
        "typescript",
        "nodejs",
        "sdk",
        "automation",
        "agentic-ai"
    ],
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "rollup -c",
        "clean": "rimraf dist && rimraf node_modules && rimraf package-lock.json",
        "docs:api": "typedoc"
    },
    "dependencies": {
        "@opentelemetry/sdk-logs": "^0.204.0",
        "axios": "^1.12.0",
        "eventemitter3": "^5.0.1",
        "file-type": "^21.0.0",
        "mime-types": "^3.0.1",
        "uuid": "^9.0.1",
        "ws": "^8.16.0",
        "zod": "^3.22.4",
        "typedoc": "^0.28.13",
        "typedoc-plugin-markdown": "^4.8.1",
        "rimraf": "^6.0.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "@types/jest": "^29.5.14",
        "@types/mime-types": "^3.0.1",
        "@types/node": "^20.11.19",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.5.10",
        "builtin-modules": "^3.3.0",
        "dotenv": "^17.2.0",
        "jest": "^29.7.0",
        "rollup": "^4.0.0",
        "rollup-plugin-dts": "^6.1.0",
        "ts-jest": "^29.4.0",
        "typescript": "^5.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UiPath/uipath-typescript.git"
    },
    "workspaces": [
        "packages/*"
    ]
}