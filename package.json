{
    "name": "@uipath/uipath-typescript",
    "version": "1.0.0-beta.2",
    "description": "UiPath TypeScript SDK",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "clean": "rm -rf dist && npm run clean --workspaces",
        "prebuild": "npm run clean",
        "build": "tsc && npm run build --workspaces",
        "build:sdk": "tsc",
        "build:cli": "npm run build --workspace=packages/cli",
        "docs": "typedoc --out docs src/index.ts",
        "prepare": "npm run build",
        "test": "jest --testPathIgnorePatterns=tests/e2e && npm run test --workspaces",
        "test:e2e": "jest --testPathPattern=tests/e2e",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
        "lint": "eslint src/**/*.ts && npm run lint --workspaces",
        "dev:cli": "npm run dev --workspace=packages/cli"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "eventemitter3": "^5.0.1",
        "file-type": "^21.0.0",
        "form-data": "^4.0.4",
        "uuid": "^9.0.1",
        "ws": "^8.16.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/form-data": "^2.2.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.19",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.5.10",
        "dotenv": "^17.2.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.4.0",
        "typescript": "^5.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UiPath/uipath-typescript.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "workspaces": [
        "packages/*"
    ]
}