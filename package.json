{
  "name": "uipath-typescript-sdk",
  "version": "0.1.2",
  "description": "TypeScript SDK for UiPath services",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "docs": "typedoc",
    "docs:generate": "typedoc --options typedoc.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "uipath",
    "sdk",
    "typescript",
    "api",
    "isomorphic"
  ],
  "author": "Raina Middha",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "builtin-modules": "^3.3.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.74.1",
    "rollup-plugin-dts": "^4.2.1",
    "ts-jest": "^28.0.3",
    "typedoc": "^0.28.4",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@types/eventemitter3": "^1.2.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^0.27.2",
    "eventemitter3": "^5.0.1",
    "ts-retry-promise": "^0.8.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browser": {
    "fs": false,
    "fs/promises": false,
    "path": false,
    "http": false,
    "https": false,
    "stream": false,
    "zlib": false
  }
}
