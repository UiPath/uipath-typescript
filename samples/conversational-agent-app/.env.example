# UiPath SDK Configuration (OAuth)
# Copy this file to .env and fill in your values

# Base URL of your UiPath instance
VITE_UIPATH_BASE_URL=https://cloud.uipath.com

# OAuth Client ID from your UiPath external application
VITE_UIPATH_CLIENT_ID=your-client-id

# OAuth Redirect URI (must match the one configured in UiPath)
VITE_UIPATH_REDIRECT_URI=http://localhost:5173/callback

# OAuth Scopes (space-separated)
VITE_UIPATH_SCOPE=OR.Execution OR.Folders OR.Jobs ConversationalAgents Traces.API

# Organization name from your UiPath account
VITE_UIPATH_ORG_NAME=your-org-name

# Tenant name from your UiPath account
VITE_UIPATH_TENANT_NAME=your-tenant-name
