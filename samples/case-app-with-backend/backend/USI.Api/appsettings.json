{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UiPathOAuth": {
    "OrganizationName": "<your-organization-name>",
    "TenantName": "<your-tenant-name>",
    "ClientId": "<your-external-app-client-id>",
    "ClientSecret": "<your-external-app-client-secret>",
    "RedirectUri": "http://localhost:3000/callback",
    "Scopes": "OR.Default offline_access openid profile email OR.Administration OR.Folders OR.Folders.Read OR.Jobs OR.Jobs.Read OR.Tasks OR.Tasks.Read OR.Tasks.Write PIMS",
    "BaseUrl": "https://cloud.uipath.com"
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:3000" ]
  }
}
